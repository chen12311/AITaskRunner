[run]
source = backend,core
omit =
    */tests/*
    */__pycache__/*
    */migrations/*
    */.venv/*
    */venv/*
branch = True
parallel = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
show_missing = True
precision = 2
fail_under = 80

[html]
directory = htmlcov
